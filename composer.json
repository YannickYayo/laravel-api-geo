{
    "name": "yannickyayo/laravel-api-geo",
    "description": "Wrapper Laravel pour communiquer avec l'API GEO du gouvernement fran√ßais.",
    "keywords": [
        "YannickYayo",
        "laravel",
        "laravel-api-geo"
    ],
    "homepage": "https://github.com/yannickyayo/laravel-api-geo",
    "license": "MIT",
    "authors": [
        {
            "name": "LEONE Yannick",
            "email": "yannick.leone@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Yannickyayo\\LaravelApiGeo\\": "src",
            "Yannickyayo\\LaravelApiGeo\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yannickyayo\\LaravelApiGeo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yannickyayo\\LaravelApiGeo\\LaravelApiGeoServiceProvider"
            ],
            "aliases": {
                "LaravelApiGeo": "Yannickyayo\\LaravelApiGeo\\LaravelApiGeoFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
